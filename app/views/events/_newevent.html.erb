<span>new event</span>
<%= form_for(@event, :html => { :class => "new_event", :multipart => true}) do |e| %>

<%= e.hidden_field :timeline_id, :value => @timeline.id %>
<%#= e.hidden_field :time, :value => "123" %>

<%= e.label :time, "time" %>
<%= e.text_field :time %><br>

<%= e.label :video, "video" %>
<%= e.text_field :video %><br>

<%= e.label :description, "description" %>
<%= e.text_field :description %><br>

<%= e.submit "post" %>

<% end %>

<script>
$('#event_time').datepicker();
</script>